{
  "name": "sharing-files-webtool",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:cesarb1392/sharing-files-webtool.git",
  "author": "Cesar <contact@cesarb.dev>",
  "license": "MIT",
  "scripts": {
    "dev": "./node_modules/.bin/next dev",
    "build": "./node_modules/.bin/next build && ./node_modules/.bin/next export -o build",
    "start": "./node_modules/.bin/next start",
    "postinstall": "./node_modules/.bin/husky install",
    "prettier": "./node_modules/.bin/prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "./node_modules/.bin/eslint --fix --ext .ts,.tsx src/**/*"
  },
  "dependencies": {
    "@chakra-ui/core": "0.8.0",
    "@chakra-ui/react": "1.6.0",
    "@emotion/react": "11",
    "@emotion/styled": "11",
    "emotion-theming": "11.0.0",
    "framer-motion": "4",
    "next": "10.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@types/node": "14.14.41",
    "@types/react": "17.0.4",
    "@types/react-dom": "17.0.1",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.22.0",
    "babel-eslint": "10.1.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "7.23.2",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "prettier": "^2.2.1",
    "typescript": "4.2.4"
  },
  "lint-staged": {
    "src/**/*.(tsx|ts)": [
      "yarn prettier",
      "yarn lint"
    ]
  }
}
